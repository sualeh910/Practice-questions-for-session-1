import java.util.Scanner;

public class PercentageCalculator {
     public static void main(String[] args) 
     {        
      System.out.println("Calculate percentage of high school graduates");
      Scanner sc = new Scanner(System.in);
      System.out.print("Enter your maximum marks :" );
      int maximum_marks = sc.nextInt() ;

      System.out.print("Enter your obtained marks : " );
      int obtained_marks = sc.nextInt() ; 

      Double percentage = 1.0*obtained_marks/maximum_marks*100; 
      System.out.println("your percentage is ; " + percentage );




    }
}
