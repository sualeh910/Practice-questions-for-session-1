import java.util.Scanner;

class OriginalPrice {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int discountedPrice;
        int discountPercent;
        int originalPrice;

        System.out.println(" Calculate original price");
        System.out.print("Enter discounted selling price: ");
        discountedPrice = sc.nextInt();

        System.out.print("Enter discount percentage: ");
        discountPercent = sc.nextInt();

        originalPrice = (discountedPrice * 100) / (100 - discountPercent);

        System.out.println("Original selling price: " + originalPrice);
    }
}
